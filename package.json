  },
  "lint-staged": {
    "{src,test}/**/*.ts*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "semi": true,
    "useTabs": true,
    "tabWidth": 4,
    "arrowParens": "always",
    "trailingComma": "none",
    "quoteProps": "as-needed",
    "trimTrailingWhitespace": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "useTabs": false,
          "tabWidth": 2
        }
      },
      {
        "files": "*.yml",
        "options": {
          "useTabs": false,
          "tabWidth": 2
        }
      }
    ]
  },
  "dependencies": {
    "prettier": "^3.3.3"
  },
