{
	"compileOnSave": false,
	"compilerOptions": {
		"moduleResolution": "node",
		"importHelpers": true,
		"target": "ES2022",
		"module": "commonjs",
		"lib": ["es2020", "dom", "es2018", "esnext.array"],
		"declaration": true,
		"sourceMap": true,
		"composite": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
